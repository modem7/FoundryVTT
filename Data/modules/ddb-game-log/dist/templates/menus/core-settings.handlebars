<form name="ddb-game-log-core-setup" class="flexcol" autocomplete="off">
    
    <nav class="sheet-tabs tabs" data-group="sections">
        <a class="item" data-tab="basic"><i class="fas fa-dice-d20"></i> Basic</a>
        <a class="item" data-tab="mapping"><i class="fas fa-cogs"></i> Mapping</a>
        <a class="item" data-tab="integrations"><i class="fas fa-plug"></i> Integrations</a>
        <a class="item" data-tab="debug"><i class="fas fa-bug"></i> Debug</a>
    </nav>

    <section class="content ddb-game-log-sheet-content">
        {{> partials/settings/basic}}
        {{> partials/settings/mapping}}
        {{> partials/settings/integrations}}
        {{> partials/settings/debug}}       
    </section>
    <br>

    <footer class="sheet-footer flexrow">
        <button type="submit" name="submit">
            <i class="far fa-save"></i> Save Changes
        </button>
    </footer>
</form>
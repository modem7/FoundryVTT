ubClassFeature":false,"limitedUse":[],"hideInBuilder":false,"hideInSheet":true,"sourceId":67,"sourcePageNumber":57,"creatureRules":[],"levelScales":[],"infusionRules":[],"spellListIds":[10797],"classId":5,"featureType":2,"sources":[{"sourceId":67,"pageNumber":null,"sourceType":1}],"affectedFeatureDefinitionKeys":[],"entityType":"class-feature","entityID":"3035763"},"version":"3.3.12","featureId":3035763,"featureName":"Additional Ranger Spells","requiredLevel":2,"class":"Ranger","classId":5,"subClass":""},"obsidian":{"source":{"type":"class","text":"Ranger"}}},"img":"icons/sundries/books/book-embossed-roots-green.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431045419,"modifiedTime":1677431045419,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"H1EChsX5VkGYwihi"}
{"name":"Channel Divinity: Knowledge of the Ages","type":"feat","system":{"description":{"value":"<p>Starting at 2nd level, you can use your Channel Divinity to tap into a divine well of knowledge. As an action, you choose one skill or tool. For 10 minutes, you have proficiency with the chosen skill or tool.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Knowledge Domain 2","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":126,"data":{"id":126,"name":"Channel Divinity: Knowledge of the Ages","prerequisite":null,"description":"<p>Starting at 2nd level, you can use your Channel Divinity to tap into a divine well of knowledge. As an action, you choose one skill or tool. For 10 minutes, you have proficiency with the chosen skill or tool.</p>","requiredLevel":2,"displayOrder":2},"version":"3.3.12","featureId":126,"featureName":"Channel Divinity: Knowledge of the Ages","requiredLevel":2,"prerequisite":null,"class":"Knowledge Domain","classId":29,"subClass":"Knowledge Domain","parentClassId":2},"obsidian":{"source":{"type":"class","text":"Knowledge Domain"}}},"img":"icons/magic/light/explosion-star-large-teal.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431042110,"modifiedTime":1677431042110,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"H27z0QKG7KLgCCET"}
{"name":"Sculpt Spells","type":"feat","system":{"description":{"value":"<p class=\"compendium-hr\">Beginning at 2nd level, you can create pockets of relative safety within the effects of your evocation spells. When you cast an evocation spell that affects other creatures that you can see, you can choose a number of them equal to 1 + the spell’s level. The chosen creatures automatically succeed on their saving throws against the spell, and they take no damage if they would normally take half damage on a successful save.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"School of Evocation 2","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":436,"data":{"id":436,"name":"Sculpt Spells","prerequisite":null,"description":"<p class=\"compendium-hr\">Beginning at 2nd level, you can create pockets of relative safety within the effects of your evocation spells. When you cast an evocation spell that affects other creatures that you can see, you can choose a number of them equal to 1 + the spell’s level. The chosen creatures automatically succeed on their saving throws against the spell, and they take no damage if they would normally take half damage on a successful save.</p>","requiredLevel":2,"displayOrder":2},"version":"3.3.12","featureId":436,"featureName":"Sculpt Spells","requiredLevel":2,"prerequisite":null,"class":"School of Evocation","classId":26,"subClass":"School of Evocation","parentClassId":8},"obsidian":{"source":{"type":"class","text":"School of Evocation"}}},"img":"icons/sundries/books/book-worn-purple.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431042989,"modifiedTime":1677431042989,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HKep9cnFCnz4iynv"}
{"name":"Eyes of the Dark","type":"feat","system":{"description":{"value":"<p class=\"Core-Styles_Core-Body\">Starting at 1st level, you have darkvision with a range of 120 feet.</p>\n<p class=\"Core-Styles_Core-Body\">When you reach 3rd level in this class, you learn the <span class=\"Serif-Character-Style_Italic-Serif\">darkness</span> spell, which doesn’t count against your number of sorcerer spells known. In addition, you can cast it by spending 2 sorcery points or by expending a spell slot. If you cast it with sorcery points, you can see through the darkness created by the spell.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Shadow Magic 1","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":750,"data":{"id":750,"name":"Eyes of the Dark","prerequisite":null,"description":"<p class=\"Core-Styles_Core-Body\">Starting at 1st level, you have darkvision&nbsp;with a range of 120 feet.</p>\r\n<p class=\"Core-Styles_Core-Body\">When you reach 3rd level in this class, you learn the <span class=\"Serif-Character-Style_Italic-Serif\">darkness</span>&nbsp;spell, which doesn’t count against your number of sorcerer spells known. In addition, you can cast it by spending 2 sorcery points or by expending a spell slot. If you cast it with sorcery points, you can see through the darkness created by the spell.</p>","requiredLevel":1,"displayOrder":1},"version":"3.3.12","featureId":750,"featureName":"Eyes of the Dark","requiredLevel":1,"prerequisite":null,"class":"Shadow Magic","classId":114,"subClass":"Shadow Magic","parentClassId":6},"obsidian":{"source":{"type":"class","text":"Shadow Magic"}}},"img":"icons/magic/perception/eye-ringed-glow-angry-small-teal.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431042773,"modifiedTime":1677431042773,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HOVaWcu8bVVgtNSv"}
{"name":"Channel Divinity: Touch of Death","type":"feat","system":{"description":{"value":"<p class=\"Core-Styles_Core-Body\">Starting at 2nd level, the cleric can use Channel Divinity to destroy another creature’s life force by touch.</p>\n<p class=\"Core-Styles_Core-Body\">When the cleric hits a creature with a melee attack, the cleric can use Channel Divinity to deal extra necrotic damage to the target. The damage equals 5 + twice his or her cleric level.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Death Domain 2","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":597,"data":{"id":597,"name":"Channel Divinity: Touch of Death","prerequisite":null,"description":"<p class=\"Core-Styles_Core-Body\">Starting at 2nd level, the cleric can use Channel Divinity to destroy another creature’s life force&nbsp;by touch.</p>\r\n<p class=\"Core-Styles_Core-Body\">When the cleric hits a creature with a melee attack, the cleric can use Channel Divinity to deal extra necrotic damage to the target. The damage equals 5 + twice his or her cleric level.</p>","requiredLevel":2,"displayOrder":3},"version":"3.3.12","featureId":597,"featureName":"Channel Divinity: Touch of Death","requiredLevel":2,"prerequisite":null,"class":"Death Domain","classId":84,"subClass":"Death Domain","parentClassId":2},"obsidian":{"source":{"type":"class","text":"Death Domain"}}},"img":"icons/skills/melee/strike-scythe-fire-green.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431042162,"modifiedTime":1677431042162,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HOWpTL61CAcuEMCW"}
{"name":"Flurry of Healing and Harm","type":"feat","system":{"description":{"value":"<p><em>11th-level Way of Mercy feature</em></p>\n<p>You can now mete out a flurry of comfort and hurt. When you use Flurry of Blows, you can now replace each of the unarmed strikes with a use of your Hand of Healing, without spending ki points for the healing.</p>\n<p>In addition, when you make an unarmed strike with Flurry of Blows, you can use Hand of Harm with that strike without spending the ki point for Hand of Harm. You can still use Hand of Harm only once per turn.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Way of Mercy 11","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":2992676,"data":{"id":2992676,"name":"Flurry of Healing and Harm","prerequisite":null,"description":"<p><em>11th-level Way of Mercy feature</em></p>\r\n<p>You can now mete out a flurry of comfort and hurt. When you use Flurry of Blows, you can now replace each of the unarmed strikes with a use of your Hand of Healing, without spending ki points for the healing.</p>\r\n<p>In addition, when you make an unarmed strike with Flurry of Blows, you can use Hand of Harm with that strike without spending the ki point for Hand of Harm. You can still use Hand of Harm only once per turn.</p>","requiredLevel":11,"displayOrder":5},"version":"3.3.12","featureId":2992676,"featureName":"Flurry of Healing and Harm","requiredLevel":11,"prerequisite":null,"class":"Way of Mercy","classId":653747,"subClass":"Way of Mercy","parentClassId":11},"obsidian":{"source":{"type":"class","text":"Way of Mercy"}}},"img":"icons/magic/life/heart-glowing-red.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431043376,"modifiedTime":1677431043376,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HPlxAK3r46aUZWGp"}
{"name":"Hybrid Transformation","type":"feat","system":{"description":{"value":"<p>Also at 3rd level, you learn to control the lycanthropic curse that courses through your veins. As a bonus action, you transform into a special hybrid form for up to 1 hour. You can speak, use equipment, and wear armor while in this form, and can revert to your normal form as a bonus action. You automatically revert to your normal form if you fall @Compendium[dnd5e.rules.w7eitkpD7QQTB6j0]{unconscious} or die.</p>\n<p>This feature replaces the <a title=\"rules\" href=\"https://www.dndbeyond.com/sources/mm/monsters-l#PlayerCharactersasLycanthropes\">rules</a> for lycanthropy in the <em><a title=\"Monster Manual\" href=\"https://www.dndbeyond.com/sources/mm\">Monster Manual</a></em>. Once you use this feature, you must finish a short or long rest before you can use it again.</p>\n<blockquote>\n<p>In the <em>Character Builder</em>, set the option for <strong>Hybrid Transformation</strong> to <strong>Hybrid Form</strong> to activate the bonuses for that form on the character sheet.</p>\n</blockquote>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Order of the Lycan 3","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":1669300,"data":{"id":1669300,"name":"Hybrid Transformation","prerequisite":null,"description":"<p>Also at 3rd level, you learn to control the lycanthropic curse that courses through your veins. As a bonus action, you transform into a special hybrid form for up to 1 hour. You can speak, use equipment, and wear armor while in this form, and can revert to your normal form as a bonus action. You automatically revert to your normal form if you fall unconscious or die.</p>\r\n<p>This feature replaces the <a title=\"rules\" href=\"https://www.dndbeyond.com/sources/mm/monsters-l#PlayerCharactersasLycanthropes\">rules</a> for lycanthropy in the <em><a title=\"Monster Manual\" href=\"https://www.dndbeyond.com/sources/mm\">Monster Manual</a></em>. Once you use this feature, you must finish a short or long rest before you can use it again.</p>\r\n<blockquote>\r\n<p>In the&nbsp;<em>Character Builder</em>, set the option for&nbsp;<strong>Hybrid Transformation</strong>&nbsp;to&nbsp;<strong>Hybrid Form</strong>&nbsp;to activate the bonuses for that form on the character sheet.</p>\r\n</blockquote>","requiredLevel":3,"displayOrder":2},"version":"3.3.12","featureId":1669300,"featureName":"Hybrid Transformation","requiredLevel":3,"prerequisite":null,"class":"Order of the Lycan","classId":360243,"subClass":"Order of the Lycan","parentClassId":357975},"obsidian":{"source":{"type":"class","text":"Order of the Lycan"}}},"img":"icons/commodities/claws/claw-bear-brown.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431042850,"modifiedTime":1677431042850,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HS6kdfRc6TWm6UIA"}
{"name":"War Magic","type":"feat","system":{"description":{"value":"<p class=\"Core-Styles_Core-Body\">Beginning at 7th level, when you use your action to cast a cantrip, you can make one weapon attack as a bonus action.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Eldritch Knight 7","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":222,"data":{"id":222,"name":"War Magic","prerequisite":null,"description":"<p class=\"Core-Styles_Core-Body\">Beginning at 7th level, when you use your action to cast a cantrip, you can make one weapon attack as a bonus action.</p>","requiredLevel":7,"displayOrder":3},"version":"3.3.12","featureId":222,"featureName":"War Magic","requiredLevel":7,"prerequisite":null,"class":"Eldritch Knight","classId":37,"subClass":"Eldritch Knight","parentClassId":10},"obsidian":{"source":{"type":"class","text":"Eldritch Knight"}}},"img":"icons/skills/melee/maneuver-greatsword-yellow.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431043114,"modifiedTime":1677431043114,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HUMDFCSsIMU5tmaj"}
{"name":"Alter Memories","type":"feat","system":{"description":{"value":"<p class=\"Core-Styles_Core-Body\">At 14th level, you gain the ability to make a creature unaware of your magical influence on it. When you cast an enchantment spell to charm one or more creatures, you can alter one creature’s understanding so that it remains unaware of being @Compendium[dnd5e.rules.w7eitkpD7QQTB6j0]{charmed}.</p>\n<p class=\"Core-Styles_Core-Body\">Additionally, once before the spell expires, you can use your action to try to make the chosen creature forget some of the time it spent @Compendium[dnd5e.rules.w7eitkpD7QQTB6j0]{charmed}. The creature must succeed on an Intelligence saving throw against your wizard spell save DC or lose a number of hours of its memories equal to 1 + your Charisma modifier (minimum 1). You can make the creature forget less time, and the amount of time can’t exceed the duration of your enchantment spell.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"School of Enchantment 14","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":434,"data":{"id":434,"name":"Alter Memories","prerequisite":null,"description":"<p class=\"Core-Styles_Core-Body\">At 14th level, you gain the ability to make a creature unaware of your magical influence on it. When you cast an enchantment spell to charm one or more creatures, you can alter one creature’s understanding so that it remains unaware of being charmed.</p>\r\n<p class=\"Core-Styles_Core-Body\">Additionally, once before the spell expires, you can use your action to try to make the chosen creature forget some of the time it spent charmed. The creature must succeed on an Intelligence saving throw against your wizard spell save DC or lose a number of hours of its memories equal to 1 + your Charisma modifier (minimum 1). You can make the creature forget less time, and the amount of time can’t exceed the duration of your enchantment spell.</p>","requiredLevel":14,"displayOrder":5},"version":"3.3.12","featureId":434,"featureName":"Alter Memories","requiredLevel":14,"prerequisite":null,"class":"School of Enchantment","classId":53,"subClass":"School of Enchantment","parentClassId":8},"obsidian":{"source":{"type":"class","text":"School of Enchantment"}}},"img":"icons/magic/control/debuff-chains-ropes-purple.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431043017,"modifiedTime":1677431043017,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HUwmC93ycNWphZTg"}
{"name":"Hurl Through Hell","type":"feat","system":{"description":{"value":"<p class=\"Core-Styles_Core-Body\">Starting at 14th level, when you hit a creature with an attack, you can use this feature to instantly transport the target through the lower planes. The creature disappears and hurtles through a nightmare landscape.</p>\n<p class=\"Core-Styles_Core-Body\">At the end of your next turn, the target returns to the space it previously occupied, or the nearest unoccupied space. If the target is not a fiend, it takes 10d10 psychic damage as it reels from its horrific experience.</p>\n<p class=\"Core-Styles_Core-Body\">Once you use this feature, you can’t use it again until you finish a long rest.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"The Fiend 14","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":404,"data":{"id":404,"name":"Hurl Through Hell","prerequisite":null,"description":"<p class=\"Core-Styles_Core-Body\">Starting at 14th level, when you hit a creature with an attack, you can use this feature to instantly transport the target through the lower planes. The creature disappears and hurtles through a nightmare landscape.</p>\r\n<p class=\"Core-Styles_Core-Body\">At the end of your next turn, the target returns to the space it previously occupied, or the nearest unoccupied space. If the target is not a fiend, it takes 10d10 psychic damage as it reels from its horrific experience.</p>\r\n<p class=\"Core-Styles_Core-Body\">Once you use this feature, you can’t use it again until you finish a long rest.</p>","requiredLevel":14,"displayOrder":5},"version":"3.3.12","featureId":404,"featureName":"Hurl Through Hell","requiredLevel":14,"prerequisite":null,"class":"The Fiend","classId":25,"subClass":"The Fiend","parentClassId":7},"obsidian":{"source":{"type":"class","text":"The Fiend"}}},"img":"icons/creatures/abilities/mouth-teeth-fire-orange.webp","effects":[],"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677431041975,"modifiedTime":1677431041975,"lastModifiedBy":"gRbX7wsbHXsd3tee"},"folder":null,"sort":0,"ownership":{"default":0,"gRbX7wsbHXsd3tee":3},"_id":"HmcO50wjPArS44GJ"}
{"name":"Magical Guidance","type":"feat","system":{"description":{"value":"<p><em>5th-level sorcerer feature</em><br />You can tap into your inner wellspring of magic to try to conjure success from failure. When you make an ability check that fails, you can spend 1 sorcery point to reroll the d20, and you must use the new roll, potentially turning the failure into a success.</p>","chat":"","unidentified":""},"source":"Tasha’s Cauldron of Everything","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":null,"attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"class","subtype":""},"requirements":"Sorcerer 5","recharge":{"value":null,"charged":false}},"flags":{"ddbimporter":{"id":3035908,"data":{"id":3035908,"definitionKey":"class-feature:3035908","entityTypeId":12168134,"displayOrder":4,"name":"Magical Guidance","description":"<p><em>5th-level sorcerer feature</em><br />You can tap into your inner wellspring of magic to try to conjure success from failure. When you make an ability check that fails, you can spend 1 sorcery point to reroll the d20, and you must use the new roll, potentially turning the failure into a success.</p>","snippet":"When you make an ability check that fails, you can spend 1 sorcery point to reroll the d20, and you must use the new roll, potentially turning the failure into a success.","activation":null,"multiClassDescription":"","requiredLevel":5,"isSubClassFeature":false,"limitedUse":[{"level":null,"uses":1}],"hideInBuilder":false,"hideInSheet":false,"sourceId":67,"sourcePageNumber":66,"creatureRules":[],"levelScales":[],"infus